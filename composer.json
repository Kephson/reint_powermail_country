{
	"name": "renolit/reint-powermail-country",
	"description": "An extended ViewHelper for the powermail country select field.",
	"license": "GPL-2.0+",
	"type": "typo3-cms-extension",
	"keywords": [
		"TYPO3 CMS"
	],
	"authors": [
		{
			"name": "Ephraim HÃ¤rer",
			"email": "mail@ephra.im",
			"homepage": "https://ephra.im",
			"role": "Developer"
		}
	],
	"homepage": "https://github.com/Kephson/reint_powermail_country",
	"support": {
		"issues": "https://github.com/Kephson/reint_powermail_country/issues"
	},
	"require": {
		"php": "^7.4 || ^8.0",
		"in2code/powermail": "^7.0 || ^8.0 || ^9.0 || ^10.4",
		"sjbr/static-info-tables": "^6.9 || ^11.5",
		"typo3/cms-core": "^10.4 || ^11.5"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "^2.28"
	},
	"autoload": {
		"psr-4": {
			"RENOLIT\\ReintPowermailCountry\\": "Classes"
		}
	},
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true,
			"ergebnis/composer-normalize": true
		}
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"extension-key": "reint_powermail_country",
			"web-dir": ".Build/web"
		}
	}
}
